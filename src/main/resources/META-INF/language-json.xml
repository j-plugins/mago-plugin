    </extensions>
    <extensions defaultExtensionNs="JavaScript">
        <JsonSchema.ProviderFactory
                implementation="com.github.xepozz.mago.config.MagoTomlJsonSchemaProviderFactory"/>

    </extensions>
</idea-plugin>
